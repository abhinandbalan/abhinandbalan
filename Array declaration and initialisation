#include <stdio.h>

int main() {
    int i, arr[5];
    arr[0] = 10;
    arr[1] = 20;
    arr[2] = 30;
    arr[3] = 40;
    arr[4] = 50;

    for (i = 0; i < 5; i++) {
        printf("%d\n", arr[i]);
    }

    return 0;
}
#include <stdio.h>

int main() {
    int i;
    int arr[5] = {10, 20, 30, 40, 50};

    for (i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
} 
